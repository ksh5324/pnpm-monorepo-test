#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/nanobundle@0.0.27/node_modules/nanobundle/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/nanobundle@0.0.27/node_modules/nanobundle/bin.mjs" "$@"
fi
